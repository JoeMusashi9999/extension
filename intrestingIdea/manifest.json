
{
  "name": "Auto Open and Accept Cookies Extension",
  "version": "1.0",
  "manifest_version": 3,
  "description": "An extension that automatically opens a page and accepts cookies.",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "cookies",
    "storage",
    "<all_urls>"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}





